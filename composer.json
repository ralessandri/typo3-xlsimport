{
  "name": "sudhaus7/xlsimport",
  "description": "(Sudhaus7) XLS Importer",
  "type": "typo3-cms-extension",
  "version": "11.5.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Frank Berger",
      "role": "Developer"
    },
    {
      "name": "Markus Hofmann",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "11.5.*",
    "phpoffice/phpspreadsheet": "1.17.*",
    "ext-json": "*",
    "php": ">=7.3"
  },
  "require-dev": {
    "typo3/cms-fluid-styled-content": "11.5.*",
    "typo3/cms-tstemplate": "11.5.*",
    "typo3/cms-backend": "11.5.*",
    "phpstan/phpstan": "*",
    "codeception/codeception": "*",
    "codeception/phpbuiltinserver": "*",
    "helhum/typo3-console": "^7.0",
    "friendsoftypo3/tt-address": "^5.3"
  },
  "suggest": {
    "friendsoftypo3/tt-address": "*"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "xlsimport",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"

    }
  },
  "autoload": {
    "psr-4": {
      "SUDHAUS7\\Xlsimport\\": "Classes/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/public/typo3conf/ext/",
      "[ -L .Build/public/typo3conf/ext/xlsimport ] || ln -snvf ../../../../. .Build/public/typo3conf/ext/xlsimport"
    ]
  }
}
